  "engines": {
    "node": "v6.3.1",
    "npm": "3.10.3"
  },
  "scripts": {
    "preinstall": "rm -r ${PWD}/node_modules/config && ln -s ${PWD}/config ${PWD}/node_modules/config",
    "start": "node scripts/start.js",
    "prebuild": "rm -rf build",
    "build": "export NODE_ENV=production; babel src -d build && webpack --config config/webpack.production.js --progress",
    "dev": "NODE_ENV=development npm start",
    "prod": "export NODE_ENV=production; npm run build && npm start",
    "test": "ava --require babel-register --fail-fast --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junkycoder/obycejnaholka.git"
  },
