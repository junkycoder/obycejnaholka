  },
  "homepage": "https://github.com/junkycoder/obycejnaholka#readme",
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "ava": "^0.16.0",
